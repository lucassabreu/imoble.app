<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="br.com.imoble.Imoble" version="1.0.0">
    <name>Imoble</name>
    <icon src="www/images/icon.png" />
    <splash src="www/images/splash.png" />
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="lucas.s.abreu@gmail.com" href="http://cordova.io">
        Lucas dos Santos Abreu
    </author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" version="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>
